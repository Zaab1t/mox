# The base URL of the OIO rest interface
rest.interface = https://${domain}



## MoxDocumentUpload Configuration ##

moxdocumentupload.amqp.interface = localhost:5672
moxdocumentupload.amqp.queueName = documentconvert
moxdocumentupload.amqp.username = guest
